{
  "name": "dexalot-contracts",
  "version": "1.2.0",
  "license": "BSD-3-Clause",
  "description": "Smart contracts in Solidity programming language for Dexalot's CLOB exchange",
  "private": true,
  "engines": {
    "node": "14.17.1"
  },
  "scripts": {
    "clean-contracts": "rimraf .openzeppelin/unknown-*.json",
    "hh-coverage": "npx hardhat coverage",
    "hh-start-clean": "yarn clean-contracts && npx hardhat node",
    "hh-test-portfolio": "npx hardhat test test/TestPortfolio.js --network localhost",
    "hh-test-orderbooks": "npx hardhat test test/TestOrderBooks.js --network localhost",
    "hh-test-tradepairs": "npx hardhat test test/TestTradePairs.js --network localhost",
    "hh-test-exchange": "npx hardhat test test/TestExchange.js --network localhost",
    "hh-test-dexalot-token": "npx hardhat test test/TestDexalotToken.js --network localhost",
    "hh-test-mock-token": "npx hardhat test test/TestMockToken.js --network localhost",
    "hh-test-airdrop-v1": "npx hardhat test test/TestAirdropV1.js --network localhost",
    "hh-test-airdrop": "npx hardhat test test/TestAirdrop.js --network localhost",
    "hh-test-vesting": "npx hardhat test test/TestTokenVesting.js --network localhost",
    "hh-test-staking": "npx hardhat test test/TestStaking.js --network localhost",
    "hh-test-dex": "npx hardhat test test/TestDexalot.js --network localhost",
    "hh-test-getNBook": "npx hardhat test test/TestGetNBook.js --network localhost",
    "hh-test-auction": "npx hardhat test test/TestAuction.js --network localhost",
    "hh-mint-dexalot-token": "npx hardhat run scripts/deploy-dexalot-token.js --network localhost",
    "hh-mint": "npx hardhat run scripts/deploy-tokens.js --network localhost",
    "hh-deploy": "npx hardhat run scripts/deploy.js --network localhost",
    "hh-deploy-dexalot-token": "npx hardhat run scripts/deploy-dexalot-token.js --network localhost",
    "hh-deploy-all": "yarn hh-mint && yarn hh-deploy",
    "hh-deploy-vesting": "npx hardhat run scripts/vesting/deploy-vesting.js --network localhost",
    "hh-deploy-airdrop-v1": "npx hardhat run scripts/airdrop/deploy-airdrop-v1.js --network localhost",
    "hh-deploy-airdrop": "npx hardhat run scripts/airdrop/deploy-airdrop.js --network localhost",
    "hh-deploy-staking": "npx hardhat run scripts/staking/deploy-staking.js --network localhost",
    "av-fund-local": "ts-node scripts/fund_c_chain.ts",
    "av-mint-local": "yarn clean-contracts && npx hardhat run scripts/deploy-tokens.js --network avash_local",
    "av-deploy-local": "yarn clean-contracts && npx hardhat run scripts/deploy.js --network avash_local",
    "av-prepare-upgrade-local": "npx hardhat run scripts/prepare-upgrade.js --network avash_local",
    "av-check-local": "npx hardhat run scripts/check.js --network avash_local",
    "av-fund-dev": "ts-node scripts/fund_c_chain.ts",
    "av-mint-dev": "yarn clean-contracts && npx hardhat run scripts/deploy-tokens.js --network avash_dev",
    "av-deploy-dev": "yarn clean-contracts && npx hardhat run scripts/deploy.js --network avash_dev",
    "av-deploy-vesting-dev": "npx hardhat run scripts/vesting/deploy-vesting.js --network avash_dev",
    "av-prepare-upgrade-dev": "npx hardhat run scripts/prepare-upgrade.js --network avash_dev",
    "av-check-dev": "npx hardhat run scripts/check.js --network avash_dev",
    "av-deploy-fuji": "yarn clean-contracts && npx hardhat run scripts/deploy.js --network fuji",
    "av-deploy-vesting-fuji": "npx hardhat run scripts/vesting/deploy-vesting.js --network fuji",
    "av-prepare-upgrade-fuji": "npx hardhat run scripts/prepare-upgrade.js --network fuji",
    "av-check-fuji": "npx hardhat run scripts/check.js --network fuji",
    "av-verify-fuji": "npx hardhat verify --network fuji",
    "av-deploy-prod": "yarn clean-contracts && npx hardhat run scripts/deploy.js --network prod",
    "av-prepare-upgrade-prod": "npx hardhat run scripts/prepare-upgrade.js --network prod",
    "av-check-prod": "npx hardhat run scripts/check.js --network prod",
    "av-verify-prod": "npx hardhat verify --network prod"
  },
  "dependencies": {
    "@chainlink/contracts": "0.3.1",
    "@ethersproject/experimental": "5.5.0",
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-etherscan": "2.1.8",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@openzeppelin/contracts": "4.3.2",
    "@openzeppelin/contracts-upgradeable": "4.3.2",
    "@openzeppelin/hardhat-upgrades": "1.12.0",
    "bignumber.js": "9.0.1",
    "chai": "4.3.4",
    "dotenv": "10.0.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.5.1",
    "hardhat": "2.6.8",
    "hardhat-contract-sizer": "2.1.1",
    "keccak256": "1.0.6",
    "merkletreejs": "0.2.27",
    "neat-csv": "6.0.1",
    "rimraf": "3.0.2",
    "solidity-coverage": "0.7.20"
  },
  "devDependencies": {
    "ts-node": "8.10.2",
    "typescript": "3.9.4"
  }
}
